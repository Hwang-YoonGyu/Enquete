<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 
<mapper namespace="org.hello.mapper.CandidateMapper">

	<insert id="insertCandidate" parameterType="domain.CandidateVO">
		insert into candidate (candidate_name, election_name, image, vote_count)
		values (#{candidate_name}, #{election_name}, #{image}, #{vote_count});
	</insert>
	
	<select id="selectCandidate" parameterType="String" resultType="domain.CandidateVO">
		select *
		from candidate
		where election_name=#{election_name};
	</select>
   

</mapper>